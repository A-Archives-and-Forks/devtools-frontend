{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "target": "ES2023",
    "lib": [
      "ES2023",
      "ES2024.Promise",
      "ESNext.Iterator",
      "ESNext.Collection",
      "ESNext.Array",
      "dom",
      "dom.iterable",
      "webworker",
      "webworker.iterable"
    ],
    "outDir": "ignored-for-vscode"
  },
  "include": [
    "../../front_end/**/*.ts",
    "../../inspector_overlay/**/*.ts",
    "../../scripts/**/*.ts",
    "../../test/**/*.ts",
    "../../extensions/**/*.ts",
    "../../extension-api/**/*.ts"
  ]
}
